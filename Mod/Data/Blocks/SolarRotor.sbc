        <Model BuildPercentUpperBound="1.00" File="Models\MA_SmartRotor\MA_SmartRotor_Base_Constr2.mwm" />
      </BuildProgressModels>
      <BlockPairName>MA_SmartRotor_Solar_Base</BlockPairName>
