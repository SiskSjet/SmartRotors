      <MaxForceMagnitude>1E9</MaxForceMagnitude>
      <DangerousTorqueThreshold>3.36E+07</DangerousTorqueThreshold>

      <RotorPart>MA_SmartRotor_Part1</RotorPart>

      <RotorDisplacementMin>-0.1</RotorDisplacementMin>
